{
  "name": "untitled",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detached": "docker-compose up -d --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:detached": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:build": "docker build -t bonoya-frontend .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t bonoya-frontend-dev .",
    "docker:run": "docker run -p 4200:4200 bonoya-frontend-dev",
    "docker:stop": "docker-compose down",
    "docker:stop:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker:clean": "docker-compose down --volumes --rmi all",
    "docker:logs": "docker-compose logs -f bonoya-frontend-dev",
    "docker:shell": "docker-compose exec bonoya-frontend-dev sh"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
